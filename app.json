{
  "expo": {
    "name": "neo-native",
    "slug": "neo-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.neonative",
      "infoPlist": {
        "NSCameraUsageDescription": "Neo needs access to your camera to take photos for upload.",
        "NSPhotoLibraryUsageDescription": "Neo needs access to your photo library to select images for upload."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.neonative"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-secure-store",
      "expo-font",
      [
        "expo-image-picker",
        {
          "photosPermission": "Neo needs access to your photos to attach images to messages.",
          "cameraPermission": "Neo needs access to your camera to take photos."
        }
      ],
      "expo-document-picker",
      [
        "expo-media-library",
        {
          "photosPermission": "Neo needs access to your photos to show recent images.",
          "isAccessMediaLocationEnabled": true
        }
      ]
    ]
  }
}
